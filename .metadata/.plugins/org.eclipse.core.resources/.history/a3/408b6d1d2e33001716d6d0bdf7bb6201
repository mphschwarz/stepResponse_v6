		NumberAxis xax = new NumberAxis("x");
		NumberAxis yax = new NumberAxis("y");
		
		
		ApplicationFrame stepResponse = new ApplicationFrame("Step Response");
        XYSplineRenderer spline = new XYSplineRenderer();
		
